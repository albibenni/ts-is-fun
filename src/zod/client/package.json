{
  "name": "busy-bee-ui",
  "version": "1.0.0",
  "private": true,
  "description": "A simple task management applicaton for the sole purpose of learning how to build type-safe applications.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
